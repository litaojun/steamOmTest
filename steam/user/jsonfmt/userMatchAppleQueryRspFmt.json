{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "message",
    "subMatchList",
    "applyInfoList"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The Code Schema",
      "default": "",
      "examples": [
        "000000"
      ],
      "pattern": "^(.*)$"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema",
      "default": "",
      "examples": [
        "成功"
      ],
      "pattern": "^(.*)$"
    },
    "subMatchList": {
      "$id": "#/properties/subMatchList",
      "type": "array",
      "title": "The Submatchlist Schema",
      "items": {
        "$id": "#/properties/subMatchList/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "matchType",
          "name",
          "state"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/subMatchList/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              1277
            ]
          },
          "matchType": {
            "$id": "#/properties/subMatchList/items/properties/matchType",
            "type": "integer",
            "title": "The Matchtype Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "name": {
            "$id": "#/properties/subMatchList/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "国赛-正常-自动化测试-勿动"
            ],
            "pattern": "^(.*)$"
          },
          "state": {
            "$id": "#/properties/subMatchList/items/properties/state",
            "type": "integer",
            "title": "The State Schema",
            "default": 0,
            "examples": [
              0
            ]
          }
        }
      }
    },
    "applyInfoList": {
      "$id": "#/properties/applyInfoList",
      "type": "array",
      "title": "The Applyinfolist Schema",
      "items": {
        "$id": "#/properties/applyInfoList/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "applyId",
          "applyCode",
          "matchId",
          "matchType",
          "matchName",
          "studentName",
          "province",
          "city",
          "country",
          "group",
          "school",
          "nationality",
          "contactName",
          "contactPhone",
          "subjectList",
          "extraInfo",
          "uuid"
        ],
        "properties": {
          "applyId": {
            "$id": "#/properties/applyInfoList/items/properties/applyId",
            "type": "integer",
            "title": "The Applyid Schema",
            "default": 0,
            "examples": [
              1471042
            ]
          },
          "applyCode": {
            "$id": "#/properties/applyInfoList/items/properties/applyCode",
            "type": "string",
            "title": "The Applycode Schema",
            "default": "",
            "examples": [
              "1471042"
            ],
            "pattern": "^(.*)$"
          },
          "matchId": {
            "$id": "#/properties/applyInfoList/items/properties/matchId",
            "type": "integer",
            "title": "The Matchid Schema",
            "default": 0,
            "examples": [
              1277
            ]
          },
          "matchType": {
            "$id": "#/properties/applyInfoList/items/properties/matchType",
            "type": "integer",
            "title": "The Matchtype Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "matchName": {
            "$id": "#/properties/applyInfoList/items/properties/matchName",
            "type": "string",
            "title": "The Matchname Schema",
            "default": "",
            "examples": [
              "国赛-正常-自动化测试-勿动"
            ],
            "pattern": "^(.*)$"
          },
          "country": {
            "$id": "#/properties/applyInfoList/items/properties/country",
            "type": "string",
            "title": "The Country Schema",
            "default": "",
            "examples": [
              "北京市"
            ],
            "pattern": "^(.*)$"
          },
          "group": {
            "$id": "#/properties/applyInfoList/items/properties/group",
            "type": "null",
            "title": "The Group Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "school": {
            "$id": "#/properties/applyInfoList/items/properties/school",
            "type": "string",
            "title": "The School Schema",
            "default": "",
            "examples": [
              "李老师"
            ],
            "pattern": "^(.*)$"
          },
          "nationality": {
            "$id": "#/properties/applyInfoList/items/properties/nationality",
            "type": "null",
            "title": "The Nationality Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "contactName": {
            "$id": "#/properties/applyInfoList/items/properties/contactName",
            "type": "string",
            "title": "The Contactname Schema",
            "default": "",
            "examples": [
              "曹操"
            ],
            "pattern": "^(.*)$"
          },
          "contactPhone": {
            "$id": "#/properties/applyInfoList/items/properties/contactPhone",
            "type": "string",
            "title": "The Contactphone Schema",
            "default": "",
            "examples": [
              "18916899938"
            ],
            "pattern": "^(.*)$"
          },
          "subjectList": {
            "$id": "#/properties/applyInfoList/items/properties/subjectList",
            "type": "null",
            "title": "The Subjectlist Schema",
            "default": null,
            "examples": [
              null
            ]
          },
          "extraInfo": {
            "$id": "#/properties/applyInfoList/items/properties/extraInfo",
            "type": "object",
            "title": "The Extrainfo Schema",
            "required": [
              "schoolEn",
              "email",
              "detailAddress",
              "postCode"
            ],
            "properties": {
              "schoolEn": {
                "$id": "#/properties/applyInfoList/items/properties/extraInfo/properties/schoolEn",
                "type": "string",
                "title": "The Schoolen Schema",
                "default": "",
                "examples": [
                  "litaojun"
                ],
                "pattern": "^(.*)$"
              },
              "email": {
                "$id": "#/properties/applyInfoList/items/properties/extraInfo/properties/email",
                "type": "string",
                "title": "The Email Schema",
                "default": "",
                "examples": [
                  "li.taojun@opg.cn"
                ],
                "pattern": "^(.*)$"
              },
              "detailAddress": {
                "$id": "#/properties/applyInfoList/items/properties/extraInfo/properties/detailAddress",
                "type": "string",
                "title": "The Detailaddress Schema",
                "default": "",
                "examples": [
                  "鹤永路599弄41号1201室"
                ],
                "pattern": "^(.*)$"
              },
              "postCode": {
                "$id": "#/properties/applyInfoList/items/properties/extraInfo/properties/postCode",
                "type": "string",
                "title": "The Postcode Schema",
                "default": "",
                "examples": [
                  "200100"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "uuid": {
            "$id": "#/properties/applyInfoList/items/properties/uuid",
            "type": "string",
            "title": "The Uuid Schema",
            "default": "",
            "examples": [
              "MTQ3MTA0MgllOTlhYmZlYi0xYWU1LTQxZDgtYTQyMi02M2JjMTA4MDI2ZDQ="
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}