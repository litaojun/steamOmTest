  "properties": {
    "code": {
      "$id": "/properties/code",
      "type": "string",
      "title": "The Code Schema ",
      "default": "",
      "examples": [
        "000000"
      ]
    },
    "message": {
      "$id": "/properties/message",
      "type": "string",
      "title": "The Message Schema ",
      "default": "",
      "examples": [
        "成功"
      ]
    },
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "itemCounts": {
          "$id": "/properties/data/properties/itemCounts",
          "type": "integer",
          "title": "The Itemcounts Schema ",
          "default": 0,
          "examples": [
            25
          ]
        },
        "currentPage": {
          "$id": "/properties/data/properties/currentPage",
          "type": "integer",
          "title": "The Currentpage Schema ",
          "default": 0,
          "examples": [
            1
