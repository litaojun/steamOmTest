{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "code": {
      "$id": "/properties/code",
      "type": "string",
      "title": "The Code Schema ",
      "default": "",
      "examples": [
        "000000"
      ]
    },
    "message": {
      "$id": "/properties/message",
      "type": "string",
      "title": "The Message Schema ",
      "default": "",
      "examples": [
        "成功"
      ]
    },
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "currentPage": {
          "$id": "/properties/data/properties/currentPage",
          "type": "integer",
          "title": "The Currentpage Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageCounts": {
          "$id": "/properties/data/properties/pageCounts",
          "type": "integer",
          "title": "The Pagecounts Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "orderList": {
          "$id": "/properties/data/properties/orderList",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/orderList/items",
            "type": "object",
            "properties": {
              "orderId": {
                "$id": "/properties/data/properties/orderList/items/properties/orderId",
                "type": "string",
                "title": "The Orderid Schema ",
                "default": "",
                "examples": [
                  "15311306036200000005112"
                ]
              },
              "title": {
                "$id": "/properties/data/properties/orderList/items/properties/title",
                "type": "string",
                "title": "The Title Schema ",
                "default": "",
                "examples": [
                  "昆明、大理、洱海骑行、丽江、泸沽湖、香格里拉、7日轻奢之旅"
                ]
              },
              "skuName": {
                "$id": "/properties/data/properties/orderList/items/properties/skuName",
                "type": "string",
                "title": "The Skuname Schema ",
                "default": "",
                "examples": [
                  "活动时间：8月4日-20日\t"
                ]
              },
              "resourceTypeId": {
                "$id": "/properties/data/properties/orderList/items/properties/resourceTypeId",
                "type": "integer",
                "title": "The Resourcetypeid Schema ",
                "default": 0,
                "examples": [
                  12
                ]
              },
              "state": {
                "$id": "/properties/data/properties/orderList/items/properties/state",
                "type": "string",
                "title": "The State Schema ",
                "default": "",
                "examples": [
                  "11"
                ]
              },
              "price": {
                "$id": "/properties/data/properties/orderList/items/properties/price",
                "type": "number",
                "title": "The Price Schema ",
                "default": 0,
                "examples": [
                  0.009999999776482582
                ]
              },
              "postPrice": {
                "$id": "/properties/data/properties/orderList/items/properties/postPrice",
                "type": "number",
                "title": "The Postprice Schema ",
                "default": 0,
                "examples": [
                  0.009999999776482582
                ]
              },
              "totalPrice": {
                "$id": "/properties/data/properties/orderList/items/properties/totalPrice",
                "type": "number",
                "title": "The Totalprice Schema ",
                "default": 0,
                "examples": [
                  0.009999999776482582
                ]
              },
              "num": {
                "$id": "/properties/data/properties/orderList/items/properties/num",
                "type": "integer",
                "title": "The Num Schema ",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "thumbUrl": {
                "$id": "/properties/data/properties/orderList/items/properties/thumbUrl",
                "type": "string",
                "title": "The Thumburl Schema ",
                "default": "",
                "examples": [
                  "http://uat-steam.opg.cn/_static/admin/images/resource/20180523163530_594379.png"
                ]
              },
              "dateStr": {
                "$id": "/properties/data/properties/orderList/items/properties/dateStr",
                "anyof":
                        {
                            "type": "string",
                            "type":null
                        },
                "title": "The Datestr Schema ",
                "default": "",
                "examples": [
                  "2018-08-03~2018-08-10"
                ]
              },
              "addressDesc": {
                "$id": "/properties/data/properties/orderList/items/properties/addressDesc",
                "anyof":{
                             "type": "string",
                             "type": null
                         } ,
                "title": "The Addressdesc Schema ",
                "default": "",
                "examples": [
                  "云南省昆明市"
                ]
              }
            }
          }
        },
        "itemCounts": {
          "$id": "/properties/data/properties/itemCounts",
          "type": "integer",
          "title": "The Itemcounts Schema ",
          "default": 0,
          "examples": [
            1
          ]
        }
      }
    }
  }
}