{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "code": {
      "$id": "/properties/code",
      "type": "string",
      "title": "The Code Schema ",
      "default": "",
      "examples": [
        "000000"
      ]
    },
    "message": {
      "$id": "/properties/message",
      "type": "string",
      "title": "The Message Schema ",
      "default": "",
      "examples": [
        "成功"
      ]
    },
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "itemCounts": {
          "$id": "/properties/data/properties/itemCounts",
          "type": "integer",
          "title": "The Itemcounts Schema ",
          "default": 0,
          "examples": [
            7
          ]
        },
        "currentPage": {
          "$id": "/properties/data/properties/currentPage",
          "type": "integer",
          "title": "The Currentpage Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "/properties/data/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema ",
          "default": 0,
          "examples": [
            20
          ]
        },
        "pageCounts": {
          "$id": "/properties/data/properties/pageCounts",
          "type": "integer",
          "title": "The Pagecounts Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "targets": {
          "$id": "/properties/data/properties/targets",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/targets/items",
            "type": "object",
            "properties": {
              "id": {
                "$id": "/properties/data/properties/targets/items/properties/id",
                "type": "integer",
                "title": "The Id Schema ",
                "default": 0,
                "examples": [
                  881
                ]
              },
              "resourceId": {
                "$id": "/properties/data/properties/targets/items/properties/resourceId",
                "type": "integer",
                "title": "The Resourceid Schema ",
                "default": 0,
                "examples": [
                  2023
                ]
              },
              "position": {
                "$id": "/properties/data/properties/targets/items/properties/position",
                "type": "string",
                "title": "The Position Schema ",
                "default": "",
                "examples": [
                  "03"
                ]
              },
              "listOrder": {
                "$id": "/properties/data/properties/targets/items/properties/listOrder",
                "type": "integer",
                "title": "The Listorder Schema ",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "picPath": {
                "$id": "/properties/data/properties/targets/items/properties/picPath",
                "type": "string",
                "title": "The Picpath Schema ",
                "default": "",
                "examples": [
                  "http://uat-steam.opg.cn/_static/admin/images/resource/20180622141625_773521.jpg"
                ]
              },
              "createTime": {
                "$id": "/properties/data/properties/targets/items/properties/createTime",
                "type": "null",
                "title": "The Createtime Schema ",
                "default": null,
                "examples": [
                  null
                ]
              },
              "updateTime": {
                "$id": "/properties/data/properties/targets/items/properties/updateTime",
                "type": "null",
                "title": "The Updatetime Schema ",
                "default": null,
                "examples": [
                  null
                ]
              },
              "title": {
                "$id": "/properties/data/properties/targets/items/properties/title",
                "type": "string",
                "title": "The Title Schema ",
                "default": "",
                "examples": [
                  "投资教育就是投资未来，而STEAM则是教育的未来"
                ]
              },
              "displayType": {
                "$id": "/properties/data/properties/targets/items/properties/displayType",
                "type": "null",
                "title": "The Displaytype Schema ",
                "default": null,
                "examples": [
                  null
                ]
              },
              "resourceType": {
                "$id": "/properties/data/properties/targets/items/properties/resourceType",
                "type": "integer",
                "title": "The Resourcetype Schema ",
                "default": 0,
                "examples": [
                  2
                ]
              },
              "specificType": {
                "$id": "/properties/data/properties/targets/items/properties/specificType",
                "type": "string",
                "title": "The Specifictype Schema ",
                "default": "",
                "examples": [
                  "素质教育"
                ]
              }
            }
          }
        }
      }
    }
  }
}