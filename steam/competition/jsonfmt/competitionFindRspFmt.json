{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "message",
    "pageContext"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The Code Schema",
      "default": "",
      "examples": [
        "000000"
      ],
      "pattern": "^(.*)$"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "string",
      "title": "The Message Schema",
      "default": "",
      "examples": [
        "成功"
      ],
      "pattern": "^(.*)$"
    },
    "pageContext": {
      "$id": "#/properties/pageContext",
      "type": "object",
      "title": "The Pagecontext Schema",
      "required": [
        "itemCounts",
        "currentPage",
        "pageSize",
        "pageCounts",
        "targets"
      ],
      "properties": {
        "itemCounts": {
          "$id": "#/properties/pageContext/properties/itemCounts",
          "type": "integer",
          "title": "The Itemcounts Schema",
          "default": 0,
          "examples": [
            6
          ]
        },
        "currentPage": {
          "$id": "#/properties/pageContext/properties/currentPage",
          "type": "integer",
          "title": "The Currentpage Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "#/properties/pageContext/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "pageCounts": {
          "$id": "#/properties/pageContext/properties/pageCounts",
          "type": "integer",
          "title": "The Pagecounts Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "targets": {
          "$id": "#/properties/pageContext/properties/targets",
          "type": "array",
          "title": "The Targets Schema",
          "items": {
            "$id": "#/properties/pageContext/properties/targets/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "matchId",
              "matchName",
              "state"
            ],
            "properties": {
              "matchId": {
                "$id": "#/properties/pageContext/properties/targets/items/properties/matchId",
                "type": "integer",
                "title": "The Matchid Schema",
                "default": 0,
                "examples": [
                  22
                ]
              },
              "matchName": {
                "$id": "#/properties/pageContext/properties/targets/items/properties/matchName",
                "type": "string",
                "title": "The Matchname Schema",
                "default": "",
                "examples": [
                  "亲子擂台赛"
                ],
                "pattern": "^(.*)$"
              },
              "state": {
                "$id": "#/properties/pageContext/properties/targets/items/properties/state",
                "type": "integer",
                "title": "The State Schema",
                "default": 0,
                "examples": [
                  2
                ]
              }
            }
          }
        }
      }
    }
  }
}