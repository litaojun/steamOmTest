{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "code": {
      "$id": "/properties/code",
      "type": "string",
      "title": "The Code Schema ",
      "default": "",
      "examples": [
        "000000"
      ]
    },
    "message": {
      "$id": "/properties/message",
      "type": "string",
      "title": "The Message Schema ",
      "default": "",
      "examples": [
        "成功"
      ]
    },
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "currentPage": {
          "$id": "/properties/data/properties/currentPage",
          "type": "integer",
          "title": "The Currentpage Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "pageSize": {
          "$id": "/properties/data/properties/pageSize",
          "type": "integer",
          "title": "The Pagesize Schema ",
          "default": 0,
          "examples": [
            10
          ]
        },
        "itemCounts": {
          "$id": "/properties/data/properties/itemCounts",
          "type": "integer",
          "title": "The Itemcounts Schema ",
          "default": 0,
          "examples": [
            1
          ]
        },
        "targets": {
          "$id": "/properties/data/properties/targets",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/targets/items",
            "type": "object",
            "properties": {
              "resourceId": {
                "$id": "/properties/data/properties/targets/items/properties/resourceId",
                "type": "integer",
                "title": "The Resourceid Schema ",
                "default": 0,
                "examples": [
                  1473
                ]
              },
              "resourceTypeId": {
                "$id": "/properties/data/properties/targets/items/properties/resourceTypeId",
                "type": "integer",
                "title": "The Resourcetypeid Schema ",
                "default": 0,
                "examples": [
                  12
                ]
              },
              "title": {
                "$id": "/properties/data/properties/targets/items/properties/title",
                "type": "string",
                "title": "The Title Schema ",
                "default": "",
                "examples": [
                  "山野少年二之无线电测向"
                ]
              },
              "saleNum": {
                "$id": "/properties/data/properties/targets/items/properties/saleNum",
                "type": "integer",
                "title": "The Salenum Schema ",
                "default": 0,
                "examples": [
                  0
                ]
              },
              "status": {
                "$id": "/properties/data/properties/targets/items/properties/status",
                "type": "string",
                "title": "The Status Schema ",
                "default": "",
                "examples": [
                  "A"
                ]
              },
              "saleOut": {
                "$id": "/properties/data/properties/targets/items/properties/saleOut",
                "type": "boolean",
                "title": "The Saleout Schema ",
                "default": false,
                "examples": [
                  false
                ]
              },
              "outOfTime": {
                "$id": "/properties/data/properties/targets/items/properties/outOfTime",
                "type": "boolean",
                "title": "The Outoftime Schema ",
                "default": false,
                "examples": [
                  true
                ]
              },
              "vendorId": {
                "$id": "/properties/data/properties/targets/items/properties/vendorId",
                "type": "integer",
                "title": "The Vendorid Schema ",
                "default": 0,
                "examples": [
                  true
                ]
              }
            }
          }
        }
      }
    }
  }
}